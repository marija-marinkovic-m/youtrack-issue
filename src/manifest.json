{
  "name": "YouTrack Issue",
  "author": {
    "name": "MM",
    "email": "marija.marinkovic@manychat.com"
  },
  "defaultLocale": "en",
  "private": true,
  "location": {
    "support": {
      "ticket_sidebar": {
        "url": "assets/iframe.html",
        "flexible": true
      }
    }
  },
  "domainWhitelist": [
    "maja.youtrack.cloud",
    "{{setting.youtrackUrl}}"
  ],
  "parameters": [
    {
      "name": "youtrackUrl",
      "type": "text",
      "required": true
    },
    {
      "name": "youtrackProject",
      "type": "text",
      "required": true
    },
    {
      "name": "apiToken",
      "type": "text",
      "secure": true,
      "required": true
    },

    {
      "name": "subdomain",
      "type": "text",
      "required": false
    }
  ],
  "version": "1.0",
  "frameworkVersion": "2.0"
}